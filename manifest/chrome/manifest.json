{
  "name": "Bitbucket File Uploader",
  "version": "1.0",
  "description": "Helps you uploading files to a bitbucket repository directly via drag'n'dop",
  "permissions": [
    "storage",
    "*://*/projects/*/repos/*/browse/*",
    "webNavigation"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "web_accessible_resources": [
    "upload_slideout.html"
  ],
  "manifest_version": 2
}
